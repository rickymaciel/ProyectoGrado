@model IEnumerable<RegionalFF.Models.Transporte>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Conductor.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Marca.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RazonSocial)
        </th>
