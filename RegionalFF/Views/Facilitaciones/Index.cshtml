@model IEnumerable<RegionalFF.Models.Facilitacion>


@{
    ViewBag.Title = "Facilitación";
    ViewBag.SubTitle = "Listado";
}


<div class="">
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <div class="x_panel">
                <div class="x_title">
                    <h2>@ViewBag.Title <small>@ViewBag.SubTitle</small></h2>
                    <ul class="nav navbar-right panel_toolbox">
                        <li>
                            <a href="@Url.Action("Create", "Facilitaciones")" class="btn btn-default btn-xs" data-toggle="tooltip" data-placement="top" title="Crear Nuevo">
                                <i class="fa fa-plus"></i>
                                <strong>Crear Nuevo</strong>
                            </a>
                        </li>
                        <li>
                            <a href="#" id="compose" class="btn btn-default btn-xs" data-toggle="tooltip" data-placement="top" title="Registrar Nuevo">
                                <i class="fa fa-plus"></i>
                                <strong>Registrar Nuevo</strong>
                            </a>
                        </li>
                    </ul>
                    <div class="clearfix"></div>
                </div>
                <div class="x_content">
                    <p class="text-muted font-13 m-b-30">
                        Administrar el Menú: <code></code>
                    </p>
                    <div class="table-responsive">
                        <table id="datatable" class="table table-striped table-condensed table-hover" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Año.Anho)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Ciudad)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Mes)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Pais)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Usuario)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Fecha)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.FechaEdicion)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Cantidad)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Estadia)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Observaciones)
                                    </th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Año.Anho)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Ciudad.Nombre)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Mes.Nombre)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Pais.Nombre)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Usuario.Email)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Fecha)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.FechaEdicion)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Cantidad)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Estadia)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Observaciones)
                                        </td>
                                        <td class="col-lg-2 btn-group">
                                            <div class="pull-right">
                                                <a class="btn btn-sm btn-warning" href="@Url.Action("Edit", "Menus", new { id = item.Id })" data-toggle="tooltip" data-placement="top" title="Editar"><i class="fa fa-edit"></i> </a>

                                                <a class="btn btn-sm btn-primary" href="@Url.Action("Details", "Menus", new { id = item.Id })" data-toggle="tooltip" data-placement="top" title="Ver Detalles"><i class="fa fa-eye"></i> </a>

                                                <a class="btn btn-sm btn-danger" href="@Url.Action("Delete", "Menus", new { id = item.Id })" data-toggle="tooltip" data-placement="top" title="Borrar"><i class="fa fa-trash-o"></i> </a>
                                            </div>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
